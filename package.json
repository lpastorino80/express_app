{
  "name": "express_app",
  "productName": "Express Pos",
  "version": "1.0.5",
  "description": "App de Express Pos",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "after-publish": "electron-builder -p never",
    "publish": "npm run package && npm run after-publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "31.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "Luis Pastorino",
    "email": "lpastorino@geocom.com.uy"
  },
  "repository": "https://github.com/lpastorino80/express_app",
  "build": {
    "appId": "com.fiserv.expressapp",
    "productName": "Express Pos",
    "compression": "normal",
    "icon": "images/icono",
    "asar": true,
    "win": {
      "target": "nsis",
      "artifactName": "Express.Pos.Setup.${version}.${ext}",
      "icon": "images/icono"
    },
    "nsis": {
      "oneClick": false,
      "uninstallDisplayName": "Express Pos",
      "shortcutName": "Express Pos"
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage",
      "icon": "images/icono.png",
      "description": "Express Pos",
      "maintainer": "Luis Pastorino <luis.pastorino@fiserv.com>",
      "artifactName": "Express.Pos.${version}.${ext}"
    },
    "mac": {
      "target": []
    },
    "directories": {
      "output": "dist"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@types/sockjs-client": "^1.5.4",
    "@types/stompjs": "^2.3.9",
    "electron-log": "^5.3.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "numeral": "^2.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serialport": "^12.0.0",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "usb": "^2.13.0",
    "usb-detection": "^4.14.2",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "websocket": "^1.0.35"
  }
}
